{
    "manifest_version": 2,
    "name": "SpecMonkey",
    "version": "0.1.0",
    "description": "Checks the domain of a page against a set of domains and displays related information.",
    "icons": {

    },
    "permissions": [
      "activeTab",
      "https://raw.githubusercontent.com/jnjaeschke/specmonkey/index/*.json"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      "config.json"
    ]
  }
