{
    "manifest_version": 2,
    "name": "SpecMonkey",
    "version": "0.1.0",
    "description": "Finds references to specification sections in the Gecko codebase.",
    "icons": {

    },
    "permissions": [
      "activeTab",
      "https://raw.githubusercontent.com/jnjaeschke/specmonkey/index/*.json"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "web_accessible_resources": [
      "config.json",
      "searchfox.png"
    ]
  }
